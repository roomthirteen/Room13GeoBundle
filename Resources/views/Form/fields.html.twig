{% block room13_geo_location_widget %}
{% set attr = attr|merge({'class': 'Room13GeoLocationWidget'}) %}
<span {{ block('widget_container_attributes') }} data-source="{{path("room13_geo_autocomplete_location",{type:"city"})}}">
      {{ form_widget(form.id) }}
      {{ form_widget(form.name) }}

      <img style="" class="Loader" src="{{ asset('bundles/room13geo/loader.gif') }}" />
</span>
{% endblock %}